    id: 2
    operand_ids: 1
  }
  program_shape {
    parameters {
      element_type: TOKEN
    }
    result {
      element_type: TOKEN
    }
    parameter_names: "Arg_0"
  }
  id: 3
  root_id: 2
}
host_program_shape {
  parameters {
    element_type: TOKEN
  }
  result {
    element_type: TOKEN
  }
  parameter_names: "Arg_0"
