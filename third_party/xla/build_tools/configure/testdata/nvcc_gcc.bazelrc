build --action_env GCC_HOST_COMPILER_PATH=/usr/bin/gcc
build --config cuda
build:cuda --repo_env HERMETIC_CUDA_VERSION="12.1.1"
build:cuda --repo_env HERMETIC_CUDA_COMPUTE_CAPABILITIES=7.5
build:cuda --repo_env HERMETIC_CUDNN_VERSION="8.6"
build --config nonccl
build --action_env LD_LIBRARY_PATH=/usr/local/nvidia/lib:/usr/local/nvidia/lib64
build --action_env PYTHON_BIN_PATH=/usr/bin/python3
build --python_path /usr/bin/python3
test --test_env LD_LIBRARY_PATH
test --test_size_filters small,medium
build --copt -Wno-sign-compare
build --build_tag_filters -no_oss,-rocm-only,-sycl-only
build --test_tag_filters -no_oss,-rocm-only,-sycl-only
test --build_tag_filters -no_oss,-rocm-only,-sycl-only
test --test_tag_filters -no_oss,-rocm-only,-sycl-only
