-  os << "### `" << op.getDialectName() << "::" << op.getCppClassName() << "`\n";
-  os << "\n";
-  os << "Creates a new " << maybeSpecLinkedOpName(op) << "operation.\n";
-  buildMethodDescription(os, "");
-  os << "\n";
-  os << "```c++\n";
-  method.writeDeclTo(os);
-  os << "```\n\n";
