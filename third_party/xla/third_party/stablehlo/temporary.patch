-  void buildMethodDescription(mlir::raw_indented_ostream& os,
-                              StringRef linePrefix);
-
-  // Write a doc string for the current builder method.
-  void buildMethodDoc(mlir::raw_indented_ostream& os, Method& method);
-
- private:
-  const Operator& op_;
-};
