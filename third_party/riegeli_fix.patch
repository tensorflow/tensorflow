Add patch to use tf build rules to prevent 'protobuf_python_genproto' error


diff --git a/riegeli/records/BUILD b/riegeli/records/BUILD
index 37db3e0..a1f42ef 100644
--- a/riegeli/records/BUILD
+++ b/riegeli/records/BUILD
@@ -1,5 +1,5 @@
-load("@com_google_protobuf//bazel:cc_proto_library.bzl", "cc_proto_library")
-load("@com_google_protobuf//bazel:proto_library.bzl", "proto_library")
+load("@local_xla//xla/tsl/platform/default:build_config.bzl", "cc_proto_library")
+load("@local_xla//xla/tsl/platform/default:build_config.bzl", "tf_proto_library")
 
 package(
     default_visibility = ["//visibility:public"],
@@ -199,10 +199,11 @@ cc_library(
     ],
 )
 
-proto_library(
+tf_proto_library(
     name = "records_metadata_proto",
     srcs = ["records_metadata.proto"],
-    deps = ["@com_google_protobuf//:descriptor_proto"],
+    protodeps = [],
+    visibility = ["//visibility:public"],
 )
 
 cc_proto_library(
     name = "records_metadata_cc_proto",
     deps = [":records_metadata_proto"],
+    visibility = ["//visibility:public"],
 )

