# Provider selection
# Options for LLM_PROVIDER: gemini, openai, anthropic, cohere
LLM_PROVIDER=gemini
# Options for EMBEDDING_PROVIDER: gemini, openai, cohere
EMBEDDING_PROVIDER=gemini

# API Keys
GOOGLE_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
COHERE_API_KEY=your_cohere_api_key_here
export LANGSMITH_API_KEY="your_langsmith_api_key"
export LANGSMITH_TRACING=true

# Model Configuration - Gemini
# GEMINI_EMBEDDING_MODEL=models/text-embedding-004
# GEMINI_LLM_MODEL=gemini-2.0-flash

# Model Configuration - OpenAI
# OPENAI_EMBEDDING_MODEL=text-embedding-3-small
# OPENAI_LLM_MODEL=gpt-4o

# Model Configuration - Anthropic
# ANTHROPIC_LLM_MODEL=claude-3-sonnet-20240229

# Model Configuration - Cohere
# COHERE_EMBEDDING_MODEL=embed-english-v3.0
# COHERE_LLM_MODEL=command

# Data storage
# CHROMA_PERSIST_DIRECTORY=DATA/chroma_store