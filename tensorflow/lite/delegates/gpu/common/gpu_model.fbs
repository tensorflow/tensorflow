include "tensorflow/lite/delegates/gpu/common/task/serialization_base.fbs";

namespace tflite.gpu.data;

table TensorDescWithId {
  desc:TensorDescriptor;
  id:int32;
}

table PairOfValueIds {
  first:int32;
  second:int32;
}

table GpuNode {
  gpu_op:GPUOperation;
  input_ids:[int32];
  output_ids:[int32];
  name:string;
}

table GpuModel {
  nodes:[GpuNode];
  tensors:[TensorDescWithId];
  const_tensors:[TensorDescWithId];
  input_ids:[int32];
  output_ids:[int32];
  input_refs:[int64];
  output_refs:[int64];
  variable_ids_and_refs:[PairOfValueIds];
}
