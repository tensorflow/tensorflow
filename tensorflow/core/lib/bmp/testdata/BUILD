        "lena.bmp",
        "rgb_small.bmp",
        "rgb_small_255.bmp",
        "rgba_small.bmp",
        "rgba_small_255.bmp",
        "grayscale_small.bmp",
        "grayscale_small_3channels.bmp",
        "grayscale_small_4channels.bmp",
    ],
    visibility = ["//visibility:public"],
)
