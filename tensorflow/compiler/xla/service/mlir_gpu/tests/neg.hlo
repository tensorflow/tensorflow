// RUN: xla-gpu-opt %s | FileCheck %s -dump-input-on-failure
HloModule Neg
ENTRY %Neg (val: f32[2,2]) -> f32[2,2] {
  %val = f32[2,2]{1,0} parameter(0)
  ROOT %neg = f32[2,2]{1,0} negate(f32[2,2]{1,0} %val)
}

